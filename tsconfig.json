{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "lib": [ "es2017", "dom" ],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "types": ["@emotion/react/types/css-prop"],
  }
}